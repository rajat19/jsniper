{
  "name": "@rajat19/jsniper",
  "version": "0.0.4",
  "description": "basic typescript template to start your coding",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "start:watch": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r templates dist"
  },
  "bin": {
    "jsniper": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajat19/jsniper.git"
  },
  "author": "Rajat Srivastava",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajat19/jsniper/issues"
  },
  "homepage": "https://github.com/rajat19/jsniper#readme",
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.10.3",
    "nodemon": "^2.0.4",
    "shx": "^0.3.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "ejs": "^3.1.5",
    "inquirer": "^7.3.3"
  }
}
